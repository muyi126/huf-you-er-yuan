<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back002"
    android:orientation="vertical" >
    <LinearLayout 
        android:padding="8dip"
        android:layout_marginTop="10dip"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button 
            android:id="@+id/b_mA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_a_icon"
            android:layout_margin="5dip"
            />
        <Button 
            android:id="@+id/b_mB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_b_icon"
            android:layout_margin="5dip"
            />
        
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button 
            android:id="@+id/b_mC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_c_icon"
            android:layout_margin="5dip"
            />
        <Button 
            android:id="@+id/b_mD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_d_icon"
            android:layout_margin="5dip"
            />
        
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button 
            android:id="@+id/b_mE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_e_icon"
            android:layout_margin="5dip"
            />
        <Button 
            android:id="@+id/b_mF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/m_duihuan"
            android:layout_margin="5dip"
            />
        
    </LinearLayout>
    <Button 
        android:id="@+id/back"
        android:layout_gravity="right"
        android:layout_marginRight="10dip"
        android:layout_width="45dip"
        android:layout_height="45dip"
        android:background="@drawable/fanhui"
        />
    
    </LinearLayout>
    

</LinearLayout>
